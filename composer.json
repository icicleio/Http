{
    "name": "icicleio/http",
    "description": "Asynchronous HTTP component for Icicle.",
    "keywords": [
        "http",
        "asynchronous",
        "async"
    ],
    "homepage": "http://icicle.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "^0.8",
        "icicleio/stream": "^0.4",
        "icicleio/socket": "^0.4",
        "icicleio/dns": "^0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.6",
        "mockery/mockery": "^0.9",
        "symfony/yaml": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Icicle\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\Http\\": "tests"
        }
    }
}
